{
  "name": "story-map-view-server",
  "version": "1.0.0",
  "description": "Proxy server for Catalyst Story Map application.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint app.js",
    "start": "npm run dev",
    "start-server": "babel-node ./src/server/index.js",
    "build:server": "webpack -p --config webpack.server.config.js",
    "build:client": "webpack -p --config webpack.client.config.js",
    "build": "npm run build:server && npm run build:client",
    "deploy": "git subtree push --prefix dist origin prod",
    "dev": "nodemon --exec \"npm run lint && npm run start-server\" --ignore src/client"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=7.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "body-parser": "^1.18.2",
    "connect-memcached": "^0.2.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "path": "^0.12.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "request": "^2.82.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "csurf": "^1.9.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "nodemon": "^1.12.1",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
